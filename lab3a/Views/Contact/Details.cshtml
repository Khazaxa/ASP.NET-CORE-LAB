@model Dictionary<int, Contact>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>To sa informacje szczegolowe na temat kontaktu</h1>
<table class="table">
    <tr>
        <th>
            Imie
        </th>
        <th>
            Email
        </th>
        <th>
            Numer Telefonu
        </th>
        <th>
            Data Urodzenia
        </th>
    </tr>
    @foreach (var item in @Model)
    {
        <tr>
            <td>
                @item.Value.Name
            </td>
            <td>
                @item.Value.Email
            </td>
            <td>
                @item.Value.Phone
            </td>
            <td>
                @item.Value.Birth
            </td>
        </tr>
    }
</table>
<a class="btn btn-outline-primary" asp-action="Index"> Powrot </a>