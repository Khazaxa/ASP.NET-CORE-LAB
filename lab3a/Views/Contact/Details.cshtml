@model List<Contact>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>To sa informacje szczegolowe na temat kontaktu</h1>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model[0].Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].Birth)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].Priority)
        </th>
        
    </tr>
    @foreach (var item in @Model)
    {
        <tr>
            <td>
                @item.Name
            </td>
            <td>
                @item.Email
            </td>
            <td>
                @item.Phone
            </td>
            <td>
                @item.Birth
            </td>
            <td>
                @item.Priority
            </td>
        </tr>
    }
</table>
<a class="btn btn-outline-primary" asp-action="Index"> Powrot </a>