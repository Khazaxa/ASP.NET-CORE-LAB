@model Contact
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>To sa informacje szczegolowe na temat kontaktu</h1>
<table class="table">
    <tr>
        <th>
            @Model.Name
        </th>
        <th>
            @Model.Email
        </th>
        <th>
            @Model.Phone
        </th>
        <th>
            @Model.Birth
        </th>
        <th>
            @Model.Priority
        </th>
        
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Name
            </td>
            <td>
                @item.Email
            </td>
            <td>
                @item.Phone
            </td>
            <td>
                @item.Birth
            </td>
            <td>
                @item.Priority
            </td>
        </tr>
    }
</table>
<a class="btn btn-outline-primary" asp-action="Index"> Powrot </a>