}

<h1>Dodaj kontakt</h1>
<form asp-controller="Contact" asp-action="Create" method="post">
    <div asp-validation-summary="All"></div>
    <input asp-for="Name" /> <br />
    <input asp-for="Email" /> <br />
    <input asp-for="Phone" /> <br />
    <input asp-for="Birth" /> <br />
    <button type="submit"> Utworz </button>
    <button type="reset"> Wyczysc </button>
    
</form> 